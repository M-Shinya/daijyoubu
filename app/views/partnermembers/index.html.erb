<h1>Listing partnermembers</h1>

<table>
  <thead>
    <tr>
      <th>Shimeicode</th>
      <th>Name</th>
	  <th>Partner</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @partnermembers.each do |partnermember| %>
      <tr>
        <td><%= partnermember.shimeicode %></td>
        <td><%= partnermember.name %></td>
        <td><%= partnermember.partner.name %></td>
		
        <td><%= link_to 'Show', partnermember %></td>
        <td><%= link_to 'Edit', edit_partnermember_path(partnermember) %></td>
        <td><%= link_to 'Destroy', partnermember, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Partnermember', new_partnermember_path %>
